{
  "name": "tamagui-vite-build-test",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.7.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "watch": "pnpm build --watch",
    "lint": "eslint .",
    "preview": "vite preview",
    "wrangler:dev": "wrangler pages dev ./dist --local",
